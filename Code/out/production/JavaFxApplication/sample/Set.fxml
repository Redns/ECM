<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Set">
    <children>
        <CheckBox layoutX="300.0" layoutY="77.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" text=" 有更新时提示我">
            <font>
                <Font size="17.0" />
            </font></CheckBox>
        <Separator prefWidth="200.0" />
        <Button fx:id="path_change" layoutX="527.0" layoutY="193.0" mnemonicParsing="false" onAction="#Change_path" prefHeight="30.0" prefWidth="160.0" text="更改存储路径">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
      <CheckBox fx:id="limit" layoutX="300.0" layoutY="135.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="227.0" text="更改路径时删除源文件夹">
         <font>
            <Font size="17.0" />
         </font>
      </CheckBox>
      <TextField fx:id="path" layoutX="300.0" layoutY="193.0" prefHeight="30.0" prefWidth="200.0" />
    </children>
</AnchorPane>
